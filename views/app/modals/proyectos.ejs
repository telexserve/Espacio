<!-- PROYECTOS REGISTER -->
<div style="background-color: whitesmoke;text-align: center;overflow-y: hidden;" data-backdrop="static" class="modal fade" id="np" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div style="margin-top: 2.5%;margin-left: 35%;overflow-y: hidden;" class="modal-dialog">
    <img data-tilt style="width: 150px;" src="assets/img/NodeX.png">
    <br><br>
      <input hidden type="text" name="usuario" value="<%= user %>">
      <input hidden type="text" name="clave" value="<%= password %>">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<img id="icon" src="assets/img/icons/projects.png"> &nbsp; Proyectos</h5>
        <button style="border: none;outline: none;" type="button" class="close" data-dismiss="modal" aria-label="Close">
          <i style="font-size: 20px;" id="close" class="bi bi-x-circle-fill"></i>
        </button>
      </div>
      <div style="height: 420px;overflow-y: auto;" class="modal-body">

        <form action="/Create_Proyect" method="post">
        <div class="row justify-content-center">
            <input hidden type="text" name="usuario" value="<%= user %>">
            <input hidden type="text" name="clave" value="<%= password %>">


            <div class="wrap-input100 validate-input" data-validate = "Usuario">
              <input type="date" name="date_start">
              &nbsp;&nbsp;&nbsp;
              <input type="date" name="date_end">
              <span class="focus-input100"></span>
            </div>
            <br><br>


            <div class="wrap-input100 validate-input" data-validate = "Usuario">
              <select required class="input100" name="lugar">
                <option hidden value="">Lugar</option>
                <option value="PVT">PVT</option>
                <option value="NVT">NVT</option>
                <option value="CAB">CAB</option>
                <option value="CAN">CAN</option>
                <option value="MZT">MZT</option>
                <option value="ACA">ACA</option>
              </select>
              <span class="focus-input100"></span>
              <span class="symbol-input100">
                <img id="icon" src="assets/img/icons/location.png">
              </span>
            </div>


            <div class="wrap-input100 validate-input" data-validate = "Usuario">
              <input required class="input100" name="codigo" type="text" placeholder="ID de Proyecto">
              <span class="focus-input100"></span>
              <span class="symbol-input100">
                <img id="icon" src="assets/img/icons/id.png">
              </span>
            </div>


            <div class="wrap-input100 validate-input" data-validate = "Usuario">
              <input required class="input100" name="proyecto" type="text" placeholder="Proyecto">
              <span class="focus-input100"></span>
              <span class="symbol-input100">
                <img id="icon" src="assets/img/icons/projects.png">
              </span>
            </div>


            <div class="wrap-input100 validate-input" data-validate = "Usuario">
              <input required class="input100" name="residente" type="text" placeholder="Residente">
              <span class="focus-input100"></span>
              <span class="symbol-input100">
                <img id="icon" src="assets/img/icons/profile.png">
              </span>
            </div>


            <div class="wrap-input100 validate-input" data-validate = "Usuario">
              <input required class="input100" name="zona" type="text" placeholder="Zona">
              <span class="focus-input100"></span>
              <span class="symbol-input100">
                <img id="icon" src="assets/img/icons/zone1.png">
              </span>
            </div>


            <div class="wrap-input100 validate-input" data-validate = "Usuario">
              <input required class="input100" name="total" type="text" placeholder="Total Asignado $">
              <span class="focus-input100"></span>
              <span class="symbol-input100">
                <img id="icon" src="assets/img/icons/totall.png">
              </span>
            </div>


            <div class="wrap-input100 validate-input" data-validate = "Usuario">
              <select required class="input100" name="anticipo">
                <option hidden value="">Anticipo</option>
                <option value="5">%5</option>
                <option value="10">%10</option>
                <option value="15">%15</option>
                <option value="20">%20</option>
                <option value="25">%25</option>
                <option value="30">%30</option>
                <option value="35">%35</option>
                <option value="40">%40</option>
                <option value="45">%45</option>
                <option value="50">%50</option>
                <option value="55">%55</option>
                <option value="60">%60</option>
                <option value="65">%65</option>
                <option value="70">%70</option>
                <option value="75">%75</option>
                <option value="80">%80</option>
                <option value="85">%85</option>
                <option value="90">%90</option>
                <option value="95">%95</option>
                <option value="100">%100</option>
              </select>
              <span class="focus-input100"></span>
              <span class="symbol-input100">
                <img id="icon2" src="assets/img/icons/per2.png">
              </span>
            </div> <br><br>


            <div class="wrap-input100 validate-input" data-validate = "Usuario">
              <input required class="input100" name="comentario" type="text" placeholder="Comentario">
              <span class="focus-input100"></span>
              <span class="symbol-input100">
                <img id="icon2" src="assets/img/icons/comment.png">
              </span>
            </div>


            <input hidden name="estatus" type="text" value="Pendiente">


            <button id="btnm" style="width: 55%;margin-top: 5%;" class="btn btn-primary" type="submit">Registrar &nbsp;&nbsp; <i style="font-size: 20px;" class="bi bi-cloud-plus-fill"></i></button>

        </div>
      </form>

      </div>
    </div>
  </div>
</div>


<!-- PROYECTOS VIEW -->
<div style="background-color: whitesmoke;text-align: center;" data-backdrop="static" class="modal fade" id="pv" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div style="margin-top: 5%;" class="modal-dialog modal-xl">
    <img data-tilt style="width: 150px;" src="assets/img/NodeX.png">
      <input hidden type="text" name="usuario" value="<%= user %>">
      <input hidden type="text" name="clave" value="<%= password %>">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<img id="icon" src="assets/img/icons/projects.png"> &nbsp; Proyectos</h5>
        <button style="border: none;outline: none;" type="button" class="close" data-dismiss="modal" aria-label="Close">
          <i style="font-size: 20px;" id="close" class="bi bi-x-circle-fill"></i>
        </button>
      </div>
      <div style="height: 350px;overflow-y: auto;" class="modal-body">

        <div class="row justify-content-center">
          <table class="table table-hover table-bordered table-dark">
            <thead>
              <tr>
                <th style="width: 200px;"> Id</th>
                <th style="width: 80000px;"> Fecha</th>
                <th style="width: 1000px;"> Lugar</th>
                <th style="width: 30000px;"> Proyecto</th>
                <th style="width: 30000px;"> Comentario</th>
                <th style="width: 30000px;"> Zona</th>
                <th style="width: 30000px;"> Residente</th>
                <th style="width: 70000px;"> Total</th>
                <th style="width: 150px;">% </th>
                <th style="width: 70000px;"> Anticipo</th>
                <th style="width: 700px;"> Estatus</th>
                <th style="width: 400000px;"></th>
              </tr>
            </thead>
            <tbody>
              <% ProyectosData.forEach(function(proyectos_data) { %>
                <tr>
                  <th scope="row"><%= proyectos_data.codigo %></th>
                  <td><%= proyectos_data.fecha_contrato %></td>
                  <td><%= proyectos_data.lugar %></td>
                  <td><%= proyectos_data.proyecto %></td>
                  <td><%= proyectos_data.comentario %></td>
                  <td><%= proyectos_data.zona %></td>
                  <td><%= proyectos_data.residente %></td>
                  <td>$ <%= parseInt(proyectos_data.total).toLocaleString() %></td>
                  <td><%= parseInt(proyectos_data.porcentaje).toLocaleString() %>%</td>
                  <td>$ <%= proyectos_data.anticipo %></td>
                  <td><%= proyectos_data.estatus %></td>
                  <td>
                    <img data-toggle="modal" data-target="#" style="width: 25px;" src="assets/img/icons/confirm3.png">
                    &nbsp;&nbsp;&nbsp;
                    <img data-toggle="modal" data-target="#pre" style="width: 25px;" src="assets/img/icons/edit9.png">
                    &nbsp;&nbsp;&nbsp;
                    <img data-toggle="modal" data-target="#prb" style="width: 25px;" src="assets/img/icons/telegram.png">
                    &nbsp;&nbsp;&nbsp;
                    <img data-toggle="modal" data-target="#prd" style="width: 25px;" src="assets/img/icons/delete3.png">
                  </td>
                </tr>
              <% }) %>
            </tbody>
          </table>
        </div>

      </div>
    </div>
  </div>
  <h5>
    <h3 data-tilt class="totals"><img style="width: 35px;" src="assets/img/icons/proyects.png"> &nbsp; Proyectos: <%= proyectos %></h3>
  </h5>
</div>


