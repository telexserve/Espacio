<!-- POR_COBRAR REGISTER -->
  <div style="background-color: whitesmoke;text-align: center;overflow-y: hidden;" data-backdrop="static" class="modal fade" id="pe" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div style="margin-top: 2.5%;margin-left: 35%;overflow-y: hidden;" class="modal-dialog">
      <img data-tilt style="width: 150px;" src="assets/img/NodeX.png">
        <input hidden type="text" name="usuario" value="<%= user %>">
        <input hidden type="text" name="clave" value="<%= password %>">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<img id="icon" src="assets/img/icons/pc2.png"> &nbsp; Por Cobrar</h5>
          <button style="border: none;outline: none;" type="button" class="close" data-dismiss="modal" aria-label="Close">
            <i style="font-size: 20px;" id="close" class="bi bi-x-circle-fill"></i>
          </button>
        </div>
        <div class="modal-body">

          <form action="/Por_Cobrar" method="post">
          <div class="row justify-content-center">
              <input hidden type="text" name="usuario" value="<%= user %>">
              <input hidden type="text" name="clave" value="<%= password %>">

              <div class="wrap-input100 validate-input" data-validate = "Usuario">
                <select required class="input100" name="lugar">
                  <option hidden value="">Lugar</option>
                  <option value="PVT">PVT</option>
                  <option value="NVT">NVT</option>
                  <option value="CAB">CAB</option>
                  <option value="CAN">CAN</option>
                  <option value="MZT">MZT</option>
                  <option value="ACA">ACA</option>
                </select>
                <span class="focus-input100"></span>
                <span class="symbol-input100">
                  <img id="icon" src="assets/img/icons/location.png">
                </span>
              </div>


              <div class="wrap-input100 validate-input" data-validate = "Usuario">
                <input required class="input100" name="codigo" type="text" placeholder="ID de Proyecto">
                <span class="focus-input100"></span>
                <span class="symbol-input100">
                  <img id="icon" src="assets/img/icons/id.png">
                </span>
              </div>


              <div class="wrap-input100 validate-input" data-validate = "Usuario">
                <input required class="input100" name="proyecto" type="text" placeholder="Proyecto">
                <span class="focus-input100"></span>
                <span class="symbol-input100">
                  <img id="icon" src="assets/img/icons/projects.png">
                </span>
              </div>


              <div class="wrap-input100 validate-input" data-validate = "Usuario">
                <input required class="input100" name="residente" type="text" placeholder="Residente">
                <span class="focus-input100"></span>
                <span class="symbol-input100">
                  <img id="icon" src="assets/img/icons/profile.png">
                </span>
              </div>


              <div class="wrap-input100 validate-input" data-validate = "Usuario">
                <input required class="input100" name="total" type="text" placeholder="Total Asignado $">
                <span class="focus-input100"></span>
                <span class="symbol-input100">
                  <img id="icon" src="assets/img/icons/total.png">
                </span>
              </div>


              <div class="wrap-input100 validate-input" data-validate = "Usuario">
                <select required class="input100" name="anticipo">
                  <option hidden value="">Anticipo</option>
                  <option value="5">%5</option>
                  <option value="10">%10</option>
                  <option value="15">%15</option>
                  <option value="20">%20</option>
                  <option value="25">%25</option>
                  <option value="30">%30</option>
                  <option value="35">%35</option>
                  <option value="40">%40</option>
                  <option value="45">%45</option>
                  <option value="50">%50</option>
                  <option value="55">%55</option>
                  <option value="60">%60</option>
                  <option value="65">%65</option>
                  <option value="70">%70</option>
                  <option value="75">%75</option>
                  <option value="80">%80</option>
                  <option value="85">%85</option>
                  <option value="90">%90</option>
                  <option value="95">%95</option>
                  <option value="100">%100</option>
                </select>
                <span class="focus-input100"></span>
                <span class="symbol-input100">
                  <img id="icon" src="assets/img/icons/ant.png">
                </span>
              </div> <br><br>


              <textarea placeholder="Comentario" style="background-color: whitesmoke;width: 75%;margin-top: 2%;" name="comentario" cols="3" rows="3"></textarea><br><br><br><br>


              <button id="btnm" style="width: 55%;margin-top: 5%;" class="btn btn-primary" type="submit">Registrar &nbsp;&nbsp; <i style="font-size: 20px;" class="bi bi-cloud-plus-fill"></i></button>

          </div>
        </form>

        </div>
      </div>
    </div>
  </div>


<!-- POR_COBRAR VIEW -->
<div style="background-color: whitesmoke;text-align: center;" data-backdrop="static" class="modal fade" id="pcv" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div style="margin-top: 5%;" class="modal-dialog modal-xl">
    <img data-tilt style="width: 150px;" src="assets/img/NodeX.png">
      <input hidden type="text" name="usuario" value="<%= user %>">
      <input hidden type="text" name="clave" value="<%= password %>">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<img id="icon" src="assets/img/icons/pc2.png"> &nbsp; Por Cobrar</h5>
        <button style="border: none;outline: none;" type="button" class="close" data-dismiss="modal" aria-label="Close">
          <i style="font-size: 20px;" id="close" class="bi bi-x-circle-fill"></i>
        </button>
      </div>
      <div style="height: 350px;overflow-y: auto;" class="modal-body">

        <div class="row justify-content-center">
          <table class="table table-hover table-bordered table-dark">
            <thead>
              <tr>
                <th style="width: 200px;" scope="col"> Id</th>
                <th style="width: 300px;" scope="col"> Lugar</th>
                <th style="width: 1000px;" scope="col"> Proyecto</th>
                <th style="width: 4500px;" scope="col"> Comentario</th>
                <th style="width: 1500px;" scope="col"> Residente</th>
                <th style="width: 700px;" scope="col"> Total</th>
                <th style="width: 700px;" scope="col"> Anticipo</th>
                <th style="width: 150px;" scope="col">% </th>
                <th style="width: 700px;" scope="col"> Faltante</th>
                <th style="width: 4000px;"></th>
              </tr>
            </thead>
            <tbody>
              <% PorCobrarData.forEach(function(porcobrar_data) { %>
                <tr>
                  <th scope="row"><%= porcobrar_data.codigo %></th>
                  <td><%= porcobrar_data.lugar %></td>
                  <td><%= porcobrar_data.proyecto %></td>
                  <td><%= porcobrar_data.comentario %></td>
                  <td><%= porcobrar_data.residente %></td>
                  <td>$ <%= parseInt(porcobrar_data.total).toLocaleString() %></td>
                  <td>$ <%= parseInt(porcobrar_data.anticipo).toLocaleString() %></td>
                  <td><%= porcobrar_data.porcentaje %>%</td>
                  <td>$ <%= parseInt(porcobrar_data.faltante).toLocaleString() %></td>
                  <td>
                    <img data-toggle="modal" data-target="#" style="width: 25px;" src="assets/img/icons/confirm3.png">
                    &nbsp;&nbsp;&nbsp;
                    <img data-toggle="modal" data-target="#pce" style="width: 25px;" src="assets/img/icons/edit9.png">
                    &nbsp;&nbsp;&nbsp;
                    <img data-toggle="modal" data-target="#pcb" style="width: 25px;" src="assets/img/icons/telegram.png">
                    &nbsp;&nbsp;&nbsp;
                    <img data-toggle="modal" data-target="#pcd" style="width: 25px;" src="assets/img/icons/delete3.png">
                  </td>
                </tr>
              <% }) %>
            </tbody>
          </table>
        </div>

      </div>
    </div>
  </div>
  <h5>
    <h3 data-tilt class="totals"><img style="width: 35px;" src="assets/img/icons/dol3.png"> &nbsp; Por Cobrar: $<%= parseInt(por_cobrar).toLocaleString() %></h3>
  </h5>
</div>


